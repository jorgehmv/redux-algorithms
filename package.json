{
  "name": "redux-algorithms",
  "description": "Algorithms visualization using redux",
  "author": "Jorge Martinez (http://github.com/jorgehmv)",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jorgehmv/redux-algorithms"
  },
  "homepage": "https://github.com/jorgehmv/redux-algorithms",
  "main": "bin/server.js",
  "scripts": {
    "start": "concurrent --kill-others \"npm run start-prod\"",
    "start-prod": "better-npm-run start-prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "better-npm-run start-dev",
    "watch-client": "better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start-prod": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "PORT": 3000
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-polyfill": "^6.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.5",
    "babel-runtime": "^6.6.1",
    "file-loader": "^0.8.5",
    "isomorphic-fetch": "^2.1.1",
    "less": "^2.6.1",
    "multireducer": "^2.0.0",
    "piping": "^0.3.0",
    "react": "^0.14.7",
    "react-bootstrap": "^0.28.3",
    "react-dom": "^0.14.7",
    "react-helmet": "^2.3.1",
    "react-redux": "^4.2.1",
    "react-router": "2.0.0-rc5",
    "react-router-redux": "^2.1.0",
    "redux": "^3.2.1",
    "redux-logger": "^2.4.0",
    "serialize-javascript": "^1.2.0",
    "serve-static": "^1.10.2",
    "superagent": "^1.7.2",
    "url-loader": "^0.5.7",
    "webpack-isomorphic-tools": "^2.2.31"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-eslint": "^5.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-typecheck": "^3.8.0",
    "babel-preset-react-hmre": "^1.0.1",
    "better-npm-run": "0.0.8",
    "bootstrap-sass": "^3.3.6",
    "bootstrap-sass-loader": "^1.0.10",
    "concurrently": "^0.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-import": "^1.0.3",
    "eslint-plugin-react": "^4.2.1",
    "express": "^4.13.3",
    "font-awesome": "^4.5.0",
    "font-awesome-webpack": "0.0.4",
    "i": "^0.3.4",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react-transform-catch-errors": "^1.0.2",
    "redux-devtools": "^3.1.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.9.11",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
